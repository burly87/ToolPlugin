package Editor;

import javafx.scene.image.Image;
import javafx.scene.layout.Region;

public class Tile extends Region {

	private float x, y, width, height;
	private Image img;
	
	public Tile(float x, float y, float width, float height, Image img)
	{
		this.x = x;
		this.y = y;
		this.width = width;
		this.height = height;
		this.img = img;
	}
	
	
	
	
}
