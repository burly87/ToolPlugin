package Editor;

import javafx.scene.image.Image;
import javafx.scene.layout.StackPane;

public class Tile extends StackPane
{

	private int x, y;
	private Image img;
	
	public Tile(int x, int y, Image img)
	{
		this.x = x;
		this.y = y;
		this.img = img;
		
		setTranslateX(x * TILE_SIZE);
		setTranslateY(y*TILE_SIZE);
	}
	
	
	
	
	
	
}
